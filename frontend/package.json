{
  "name": "sysml-mvp-frontend",
  "version": "0.1.0",
  "description": "SysML v2建模平台MVP前端",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "test": "vitest --config vite.config.test.ts",
    "test:coverage": "vitest run --coverage --config vite.config.test.ts",
    "test:ui": "vitest --ui --config vite.config.test.ts",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "format": "prettier --write \"src/**/*.{ts,tsx,css,md}\"",
    "type-check": "tsc --noEmit",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "e2e": "start-server-and-test dev http://localhost:5173 cypress:run",
    "e2e:headed": "start-server-and-test dev http://localhost:5173 cypress"
  },
  "dependencies": {
    "@ant-design/icons": "^5.6.1",
    "antd": "^5.27.1",
    "axios": "^1.6.2",
    "classnames": "^2.3.2",
    "dagre": "^0.8.5",
    "dayjs": "^1.11.10",
    "elkjs": "^0.8.2",
    "lodash": "^4.17.21",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.20.1",
    "reactflow": "^11.10.1",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@cypress/react": "^9.0.1",
    "@cypress/webpack-dev-server": "^5.1.1",
    "@testing-library/jest-dom": "^6.1.5",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/dagre": "^0.7.52",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@types/uuid": "^9.0.7",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.0.4",
    "cypress": "^15.0.0",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "jsdom": "^23.0.1",
    "prettier": "^3.1.1",
    "sass": "^1.69.5",
    "start-server-and-test": "^2.0.13",
    "typescript": "^5.3.3",
    "vite": "^5.0.8",
    "vitest": "^1.0.4"
  }
}
